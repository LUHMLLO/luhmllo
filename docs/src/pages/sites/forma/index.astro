---
import ShellLayout from '../../../layouts/app.astro';
import ViewFrame from '../../../layouts/frame.astro';
---

<ShellLayout>
	<ViewFrame>
		<header>
			<figure data-is='background'>
				<img
					src='https://images.unsplash.com/photo-1728443433557-3fc9e37b58c2?q=80&w=690&auto=format&fit=crop&ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D'
					alt=''
				/>
				<div
					data-is='layer'
					data-variant='over'
					style='background-color: hsl(0 0 0 / 75%); backdrop-filter: blur(0.5ex); visibility: visible;'>
				</div>
			</figure>
		</header>

		<section></section>
	</ViewFrame>
</ShellLayout>
